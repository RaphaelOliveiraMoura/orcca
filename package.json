{
  "name": "orcca-back-end",
  "version": "1.0.0",
  "main": "src/index.js",
  "repository": "https://github.com/RaphaelOliveiraMoura/orcca-back-end.git",
  "author": "Raphael <raphaeldeoliveiramoura@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "node .",
    "db:prepare": "yarn sequelize db:drop || echo 'Database dropped' && yarn sequelize db:create && npm run db:migrate && npm run db:seed",
    "db:migrate": "yarn sequelize db:migrate",
    "db:migrate:undo": "yarn sequelize db:migrate:undo:all",
    "db:seed": "yarn sequelize db:seed:all",
    "db:seed:undo": "yarn sequelize db:seed:undo:all",
    "test": "export ENV=test && npm run db:prepare && jest",
    "test:watch": "watch 'npm run test' ./__tests__"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "crypto-js": "^3.1.9-1",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "^1.6.5",
    "sequelize": "^5.15.0"
  },
  "devDependencies": {
    "jest": "^24.9.0",
    "supertest": "^4.0.2",
    "sequelize-cli": "^5.5.0",
    "watch": "^1.0.2"
  }
}
